{
  "compilerOptions": {
    "module": "CommonJS",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "target": "es6",
    "strict": true,
    "sourceMap": true,
    "outDir": "dist",
    "declaration": true,
    "lib": [
      "esnext",
    ]
  },
  "include": [
    "src"
  ],
  "exclude": [
    "node_modules"
  ],
}
